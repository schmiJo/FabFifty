{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Nuxt App",
            "program": "${workspaceFolder}/.output/server/index.mjs",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node",
            "env": {
                "NODE_ENV": "development"
            },
            "preLaunchTask": "nuxt:build"
        }
    ]
}